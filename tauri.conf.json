        "devPath": "./src",
        "distDir": "./src",
        "withGlobalTauri": true
    },
    "package": {
        "productName": "WootingProfileSwitcher",
        "version": "1.5.2"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "shell": {
                "all": false,
                "open": true
            }
        },
        "bundle": {
            "active": true,
            "targets": "all",
            "identifier": "com.shaybox.wooting-profile-switcher",
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
